{
    "version": "2.0.0",
    "command": "g++",
    "args": [
        "-g",                
        "-Wall",
        "-o",
        "tsh",
        "tsh.c",
        "myspin.c" ,
        "mysplit.c",
        "mystop.c",
        "myint.c",
    ], // 编译命令参数
    "problemMatcher": {
        "owner": "cpp",
        "fileLocation": [
            "relative",
            "${workspaceRoot}"
        ],
        "pattern": {
            "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "severity": 4,
            "message": 5
        }
    }
}